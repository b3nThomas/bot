{
  "name": "bot",
  "version": "1.0.0",
  "description": "A bot that you can interact with",
  "main": "index.js",
  "scripts": {
    "build": "npm run transpile",
    "build:client": "npm run build && npm run webpack",
    "test": "npm run build && npx ava",
    "transpile": "tsc -p .",
    "watch": "nodemon -e ts --exec \"<command>\"",
    "watch:client": "nodemon -e ts --exec \"npm run build:client\"",
    "webpack": "npx webpack --config webpack.config.js"
  },
