{
  "name": "bot",
  "version": "1.0.0",
  "description": "A bot that you can interact with",
  "main": "index.js",
  "scripts": {
    "build": "npm run lint && npm run transpile",
    "lint": "npx tslint -p tsconfig.json",
    "lint:fix": "npx tslint --fix -p tsconfig.json",
    "test": "npm run build && npx ava",
    "transpile": "tsc -p .",
    "watch": "nodemon -e ts --exec \"<command>\"",
    "webpack": "npx webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/b3nThomas/bot.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/b3nThomas/bot/issues"
  },
  "homepage": "https://github.com/b3nThomas/bot#readme",
  "devDependencies": {
    "nodemon": "^1.19.1",
    "tslint": "^5.19.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.5.3",
    "webpack": "^4.39.2"
  }
}
