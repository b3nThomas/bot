  "devDependencies": {
    "@types/node": "^12.7.2",
    "nodemon": "^1.19.1",
    "typescript": "^3.5.3",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "@types/dom-mediacapture-record": "^1.0.1",
    "axios": "^0.19.0"
